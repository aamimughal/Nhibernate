<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Sample.CustomerService" namespace="Sample.CustomerService.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Student" table="student" lazy="true" >
    <id name="id">
      <column name="id" />
      <generator class="identity"/>
    </id>
    
    <id name="StudentId" column="student id" />
    <property name="Studentname">
      <column name="studentname" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Studentemail">
      <column name="studentemail" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Teacherid">
      <column name="teacherid" sql-type="varchar(45)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>